extends layout

block content
  h1= title
  h2(id='roomName')
  div(id='chat-panel' class='panel panel-default')
    ul(id='messages')
    p 
    div(id='chatbox-wrapper')
      form(
        action="",
        id="chatForm"
        )
        input(
          id="m",
          class='chatbox',
          autocomplete="off",
          placeholder="Enter a message"
        )